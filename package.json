{
  "name": "mr.koa",
  "version": "0.9.5",
  "description": "The most beautiful koa boilerplate.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts --fix",
    "test": "npm run specs && jest",
    "test:watch": "npm run specs && jest --watch",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start": "node index.js",
    "create_db": "sequelize db:create",
    "migrate": "sequelize db:migrate",
    "build": "npm run specs && tsc && npm run migrate",
    "specs": "cp specs/header.yaml.tpl specs/header.yaml && mkdir -p src/types/Api && tinyspec --json --add-nulls -o specs/ && sw2dts specs/openapi.json -o src/types/Api/index.d.ts -n Api",
    "onlyhunter": "dot-only-hunter test",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@types/bluebird": "^3.5.30",
    "@types/json-schema": "^7.0.6",
    "@types/validator": "^13.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "glob": "^7.1.6",
    "i18n": "^0.12.0",
    "json-schema-deref-sync": "^0.14.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-qs": "^3.0.0",
    "koa-router": "^8.0.8",
    "lodash": "^4.17.20",
    "nyc": "^15.0.1",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.4",
    "sw2dts": "^2.6.3",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/faker": "^5.1.5",
    "@types/glob": "^7.1.3",
    "@types/i18n": "^0.12.0",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-qs": "^2.0.0",
    "@types/koa-router": "^7.4.0",
    "@types/koa__cors": "^3.0.2",
    "@types/lodash": "^4.14.165",
    "@types/node": "^13.13.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "ajv": "^6.12.6",
    "dot-only-hunter": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.0.1",
    "tinyspec": "^2.4.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "koa",
    "boilerplate"
  ],
  "author": "Ilia Donskikh",
  "license": "ISC"
}
